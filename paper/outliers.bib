issn = {1939-2168},
journal = {Journal of Counseling Psychology},
keywords = {2016,aims to capture,autin,blustein,decent work,diemer,duffy,instrument development,of adults across the,of working theory,psychology of working,pwt,range of,the career development process,the recently developed psychology},
