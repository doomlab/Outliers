number = {4},
pages = {606--621},
title = {{Circumventing resistance: Using values to indirectly change attitudes.}},
url = {https://www.researchgate.net/profile/Duane{\_}Wegener/publication/228098868{\_}Circumventing{\_}Resistance{\_}Using{\_}Values{\_}to{\_}Indirectly{\_}Change{\_}Attitudes/links/575075d908ae1f765f9318d6/Circumventing-Resistance-Using-Values-to-Indirectly-Change-Attitudes.pdf http://do},
