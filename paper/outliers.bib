keywords = {1985,1993,1997,barnett,bass,coach behaviors have been,critical role in shaping,e,everett,experiences of athletes,g,has recently been incorporated,identified as playing a,into,leadership,multilevel analyses,narcissism,personality,smith,smoll,sport,the,transformational leadership,youth},
