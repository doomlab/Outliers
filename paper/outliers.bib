}
@article{Vogel2010,
abstract = {Public and self-stigmas have been implicated as factors in the underutilization of individual counseling. However, group counseling is also underutilized, and yet scholars know very little about the role of different types of stigma on attitudes toward seeking group counseling. Therefore, the current study examined the relationships between public and self-stigma and attitudes toward group counseling among a sample of 491 U.S. college students. Results of structural equation modeling analyses indicated that public stigma is internalized as self-stigma and self-stigma is then negatively related to attitudes toward group counseling. Furthermore, public stigma and self-stigma explained 52{\%} of the variance in attitudes toward seeking help. (PsycINFO Database Record (c) 2013 APA, all rights reserved) (journal abstract)},
