number = {5},
pages = {508--515},
pmid = {26949926},
title = {{Relation of white-matter microstructure to reading ability and disability in beginning readers.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26949926},
volume = {31},
year = {2016}
}
