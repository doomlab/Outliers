journal = {Journal of Educational Psychology},
keywords = {age or being born,are usually considered to,be risk,differential susceptibility,for gestational,late preterm,mild perinatal adversities,randomized control trial,short- and long-term effects,such as being small,web-based early literacy intervention},
number = {2},
pages = {337--349},
title = {{Differential susceptibility to early literacy intervention in children with mild perinatal adversities: Short- and long-term effects of a randomized control trial.}},
