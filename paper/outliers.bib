@article{Vadasy2012,
abstract = {The current study follows a sample of lower skilled language minority (LM) and native English-speaking (non-LM) students who participated in an efficacy trial of a kindergarten phonics-based intervention. Follow-up procedures allowed 93{\%} of the original sample to be retained for simple treatment effects modeling (N = 78 LM and N = 59 non-LM) and 72{\%} to be retained for classroom instruction modeling (N = 62 LM and N = 44 non-LM). Simple treatment effects on longer term outcomes were detected on word reading, spelling, and comprehension outcomes for LM students (approximate effect sizes averaged 0.27); comparatively, treatment effects for non-LM students were detected on all outcomes, including fluency (approximate effect sizes averaged 0.54). Instructional model results showed that greater time in Grade 1 word study instruction and Grade 2 meaning instruction was associated with higher reading scores for LM students at the end of second grade, irrespective of experimental condition. For non-LM children, greater time in Grade 1 meaning instruction was connected with higher reading scores at the end of Grade 2, irrespective of experimental condition. Finally, kindergarten intervention effects tended to be greater for both LM and non-LM students who received more Grade 1 word study instruction and more Grade 2 meaning instruction. Limitations and practical implications are discussed. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
author = {Vadasy, Patricia F. and Sanders, Elizabeth A.},
doi = {10.1037/a0028163},
isbn = {0022-0663},
issn = {1939-2176},
journal = {Journal of Educational Psychology},
keywords = {a successful,acquisition of early reading,as poor readers are,children who begin school,english learner,intervention,likely to remain,longitudinal studies,many studies have shown,randomized trial,reading,skills is important for,that,trajectory of reading development},
number = {4},
pages = {987--1005},
title = {{Two-year follow-up of a kindergarten phonics intervention for English learners and native English speakers: Contextualizing treatment impacts by classroom literacy instruction.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028163},
volume = {104},
year = {2012}
}
