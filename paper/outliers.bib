volume = {73},
year = {2017}
}
@article{Vissers2017,
abstract = {Objective Treatment effects in psychotherapy outcome research are generally based on the reduction of symptoms. Standard inclusion of other beneficial treatment effects such as remoralization (increase of hope, self-efficacy, well-being) might lead to more elaborate findings in the field of psychotherapy. On the other hand, it is also possible that symptom reduction and remoralization always go hand in hand in the experience of patients. The present study sought to experimentally test this assumption. Method A total of 78 patients suffering from panic disorder were randomly assigned to brief remoralization treatment, brief exposure treatment, or waiting list (WL). Results Both treatments increased remoralization and both reduced symptoms of panic disorder as compared to WL. Conclusion It is unlikely that patients experience remoralization without symptom reduction or symptom reduction without remoralization. These findings do not favor the assumption that conclusions within psychotherapy outcome research are flawed because of its heavy reliance on measurements of symptom reduction.},
author = {Vissers, Wiede and Keijsers, Ger P.J. and Kampman, Mirjam and Hendriks, Gert-Jan and Rijnders, Paul and Hutschemaekers, Giel J.M.},
doi = {10.1002/jclp.22380},
