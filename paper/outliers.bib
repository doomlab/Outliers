number = {4},
pages = {833--845},
pmid = {22329531},
title = {{The Revised Child Anxiety and Depression Scale-Short Version: Scale reduction via exploratory bifactor modeling of the broad anxiety factor.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0027283},
volume = {24},
