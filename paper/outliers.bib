keywords = {DERS,Eating Disorders,Emotion Regulation,Reliability,Validity},
number = {9},
pages = {989--1007},
pmid = {22653763},
title = {{Cultural adaptation of the difficulties in emotion regulation scale: Reliability and validity of an Italian version}},
volume = {68},
year = {2012}
}
