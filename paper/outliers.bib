@article{Arrieta2017,
abstract = {Background: Depressive disorders are frequently under diagnosed in resource-limited settings because of lack of access to mental health care or the inability of healthcare providers to recognize them. The Patient Health Questionnaire (PHQ)-2 and the PHQ-9 have been widely used for screen- ing and diagnosis of depression in primary care settings; however, the validity of their use in rural, Spanish-speaking populations is unknown. Method: We used a cross-sectional design to assess the psychometric properties of the PHQ-9 for depression diagnosis and estimated the sensitivity and specificity of the PHQ-2 for depression screening. Data were collected from 223 adults in a rural com- munity of Chiapas, Mexico, using the PHQ-2, the PHQ-9, and the World Health Organization Quality of Life BREF Scale (WHOQOL- BREF). Results: Confirmatory factor analysis suggested that the 1-factor structure fit reasonably well. The internal consistency of the PHQ-9 was good (Cronbach's alpha{\textgreater}= 0.8) overall and for subgroups defined by gender, literacy, and age. The PHQ-9 demonstrated good predictive validity: Participants with a PHQ-9 diagnosis of depression had lower quality of life scores on the overallWHOQOL-BREF Scale and each of its domains. Using the PHQ-9 results as a gold standard, the optimal PHQ-2 cutoff score for screening of depressionwas 3 (sensitivity 80.00{\%}, speci- ficity 86.88{\%}, area under receiver operating characteristic curve=0.89; 95{\%} confidence interval [0.84, 0.94]). Conclusion: The PHQ-2 and PHQ-9 demonstrated good psychometric properties, suggest- ing their potential benefit as tools for depression screening and diagnosis in rural, Spanish-speaking populations.},
author = {Arrieta, Jafet and Aguerrebere, Mercedes and Raviola, Giuseppe and Flores, Hugo and Elliott, Patrick and Espinosa, Azucena and Reyes, Andrea and Ortiz-Panozo, Eduardo and Rodriguez-Gutierrez, Elena G. and Mukherjee, Joia and Palazuelos, Daniel and Franke, Molly F.},
doi = {10.1002/jclp.22390},
issn = {00219762},
journal = {Journal of Clinical Psychology},
keywords = {Mental health care,Primary care,Validation},
month = {sep},
number = {9},
pages = {1076--1090},
pmid = {28195649},
title = {{Validity and utility of the Patient Health Questionnaire (PHQ)-2 and PHQ-9 for screening and diagnosis of depression in rural Chiapas, Mexico: A cross-sectional study}},
url = {http://doi.wiley.com/10.1002/jclp.22390},
volume = {73},
year = {2017}
}
@article{Resnik2017,
abstract = {OBJECTIVE: Veterans Affairs (VA) is expanding peer support. Research is limited on Veterans' perspective on benefits from peer services. We describe homeless Veteran perceptions of value and examine characteristics associated with benefit. METHOD: From a sample of Veterans in a multisite randomized control trial, we studied addition of peers in VA Primary Care and homeless-oriented primary care clinics. We used qualitative methods to study the perceptions of peer services among a subsample of homeless Veterans. Quantitative methods were used to validate findings in both samples. RESULTS: Sixty-five percent of the subsample and 83{\%} of the full sample benefited from a peer mentor. Participants who benefited had more peer visits and minutes of intervention (p{\textless}.05), were more likely to be minority, and were less likely to have posttraumatic stress disorder. CONCLUSION: The majority of Veteran participants in this study benefited from receiving peer mentor intervention. African American Veterans were more likely to benefit and Veterans with PTSD were less likely to benefit. Client endorsement of the peer's role influenced outcomes.},
author = {Resnik, Linda and Ekerholm, Sarah and Johnson, Erin E. and Ellison, Marsha L. and O'Toole, Thomas P.},
doi = {10.1002/jclp.22407},
issn = {00219762},
