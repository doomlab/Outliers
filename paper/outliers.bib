journal = {Journal of Sport and Exercise Psychology},
keywords = {continuum,latent profile analysis,perceived locus of causality,self-determination continuum},
month = {dec},
number = {6},
pages = {612--630},
pmid = {24911322},
title = {{Students' motivational profiles in the physical education context}},
url = {http://journals.humankinetics.com/doi/10.1123/ijspp.2015-0012{\%}5Cnhttp://journals.humankinetics.com/doi/10.1123/jsep.2016-0153 http://journals.humankinetics.com/doi/10.1123/jsep.2016-0153},
