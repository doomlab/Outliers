issn = {1939-1285},
journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
keywords = {conditions in which ques-,e,extensive research has documented,eyewitness memory,forgetting,g,have delete-,loftus,memory over time,memory performance,repeated recall,rious effects on subsequent,see,testing effect,tioning eyewitnesses after exposure,to an event can},
