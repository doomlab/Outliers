journal = {Journal of Consulting and Clinical Psychology},
keywords = {alcohol,claes,deliberate attempts to cause,deliberate self-harm,dose-response,dsh,fall under the broad,havertape,irrespective of lethal intent,muehlenkamp,nssi,physical harm or injury,self-aggression paradigm,to oneself,umbrella of},
