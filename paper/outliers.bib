journal = {Behavior Research Methods},
keywords = {Fuzzy-Trace Theory,Gis,Intelligent Tutoring System,and growing,fuzzy-trace theory,gist,health,intelligent tutoring,ment in preventing obesity,obesity,of a new approach,system,to cognitive engage-,we report the results,which is a serious},
