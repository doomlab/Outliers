journal = {Psychology, Public Policy, and Law},
keywords = {*Child Abuse,*Credibility,*Cross Examination,*Interrogation,*Sexual Abuse},
number = {2},
pages = {200--210},
title = {{Challenging the credibility of alleged victims of child sexual abuse in Scottish courts.}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}PAGE=reference{\&}D=psyc13{\&}NEWS=N{\&}AN=2017-02990-001},
volume = {23},
year = {2017}
