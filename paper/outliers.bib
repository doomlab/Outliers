journal = {Contemporary Educational Psychology},
keywords = {Arithmetic,Comorbidity,Covariation,Development,Reading},
month = {oct},
pages = {131--140},
title = {{Covariation between reading and arithmetic skills from Grade 1 to Grade 7}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0361476X17302771},
volume = {51},
year = {2017}
}
@article{Schnotz2017,
abstract = {375 students from 24 randomly selected classes of a three-tier secondary school system were tested in a longitudinal study for their text-picture integration and pure reading competence as well as verbal and spatial intelligence at grades 5–7. Data were analyzed according to the integrated model of text and picture comprehension using hierarchical linear modeling techniques. Results indicate that text-picture integration comprises higher spatial cognitive demands than pure reading. School tiers differed in terms of competency levels, but also in terms of growth rates of text-picture integration competence. Differences between lower tiers and higher tiers for text-picture integration competence became smaller from grade to grade, whereas developmental trajectories of reading competence ran parallel to each other. The study reveals that the skills for the conjoint processing of text and pictures develop in a way that might help especially poorer students in lower school tiers to catch up with their mates in higher tiers as compared to the competence of pure reading. Text-picture integration seems to provide gradually better opportunities for less capable learners to compensate for previous lags in their learning.},
author = {Schnotz, Wolfgang and Wagner, Inga and Ullrich, Mark and Horz, Holger and McElvany, Nele},
doi = {10.1016/j.cedpsych.2017.06.003},
issn = {0361476X},
journal = {Contemporary Educational Psychology},
keywords = {Conjoint processing of texts and pictures,Reading,School tiers,Spatial intelligence,Text-picture integration,Verbal intelligence},
month = {oct},
