@article{Rossano2012,
abstract = {We investigated 1-year-old infants' ability to infer an adult's focus of attention solely on the basis of her voice direction. In$\backslash$r$\backslash$nStudies 1 and 2, 12- and 16-month-olds watched an adult go behind a barrier and then heard her verbally express excitement$\backslash$r$\backslash$nabout a toy hidden in one of two boxes at either end of the barrier. Even though they could not see the adult, infants of$\backslash$r$\backslash$nboth ages followed her voice direction to the box containing the toy. Study 2 showed that infants could do this even when$\backslash$r$\backslash$nthe adult was positioned closer to the incorrect box while she vocalized toward the correct one (and thus ruled out the$\backslash$r$\backslash$npossibility that infants were merely approaching the source of the sound). In Study 3, using the same methods as in Study$\backslash$r$\backslash$n2, we found that chimpanzees performed the task at chance level. Our results show that infants can determine the focus of$\backslash$r$\backslash$nanother person's attention through auditory information alone—a useful skill for establishing joint attention.},
