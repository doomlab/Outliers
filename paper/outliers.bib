journal = {Journal of Counseling Psychology},
keywords = {a leader in,coping,counseling psychology journals have,gender identity,grounded theory,indi-,psychology has emerged as,published special,qualitative,research and practice with,the field of counseling,trans,trans and gender nonconforming,viduals},
