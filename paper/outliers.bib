pmid = {22905770},
title = {{Bidirectionality, mediation, and moderation of metaphorical effects: The embodiment of social suspicion and fishy smells.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0029708},
volume = {103},
