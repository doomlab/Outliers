journal = {Journal of Clinical Psychology},
keywords = {Asian Americans,Client factors,Immigrants,Outcomes,Psychotherapy},
number = {12},
pages = {1287--1302},
pmid = {22836681},
title = {{Client predictors of short-term psychotherapy outcomes among Asian and white American outpatients}},
volume = {68},
year = {2012}
