pages = {1005--1019},
pmid = {28125253},
title = {{Sex differences in mental rotation tasks: Not just in the mental rotation process!}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28125253{\%}0Ahttp://doi.apa.org/getdoi.cfm?doi=10.1037/xlm0000370},
volume = {43},
year = {2017}
}
