journal = {Psychological Science},
keywords = {16,17,29,3,adult development,aging,and recall arbitrary word,associative processes,behavioral assessment,bilingualism,e,g,open data,open materials,pairs,received 1,revision accepted 4,the ability to learn},
number = {8},
pages = {1171--1179},
pmid = {28700267},
title = {{The mismeasurement of mind: Life-span changes in Paired-Associate-Learning Scores reflect the “cost” of learning, not cognitive decline}},
