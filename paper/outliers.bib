number = {1},
pages = {34--58},
pmid = {28447837},
title = {{The state of social and personality science: Rotten to the core, not so bad, getting better, or getting worse?}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/pspa0000084},
volume = {113},
