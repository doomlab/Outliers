@article{Smith2011,
abstract = {Researchers have suggested that one function of touch in mixed-sex$\backslash$ninteractions is to exert influence over another person. Yet theories$\backslash$noffer different explanations as to when women and men will use touch as$\backslash$nan influence strategy. The gender politics hypothesis proposes that men$\backslash$ntouch more as a way to maintain inequalities present in society. In$\backslash$nturn, the dyadic power theory proposes that both women and men will$\backslash$ntouch more depending on their goals in a given situation. The person$\backslash$ninitiating a topic of disagreement is more likely to touch in order to$\backslash$ntry and influence the other person to agree with his or her position.$\backslash$nHowever, researchers have rarely examined the different assertions of$\backslash$nthese theories within intimate relationships. The present study, with 67$\backslash$nmarried heterosexual couples, was designed to provide an initial test of$\backslash$nthese theories. The authors focused on four types of touch across two$\backslash$nproblem-solving topics: one chosen by each spouse. Consistent with the$\backslash$ndyadic power theory, results indicated that when couples discussed$\backslash$ntopics chosen by wives, wives exhibited more touches. However, no$\backslash$ndifferences in these forms of touch emerged when couples discussed$\backslash$ntopics chosen by husbands. Implications for marital counseling and$\backslash$nresearch are discussed.},
author = {Smith, Joann C. Seeman and Vogel, David L. and Madon, Stephanie and Edwards, Sarah R.},
doi = {10.1177/0011000010385849},
isbn = {0011-0000,00110000},
issn = {0011-0000},
journal = {The Counseling Psychologist},
month = {jul},
number = {5},
pages = {764--787},
title = {{The power of touch: Nonverbal communication within married dyads}},
url = {http://journals.sagepub.com/doi/10.1177/0011000010385849},
volume = {39},
year = {2011}
}
@article{Rice2012,
abstract = {The authors tested a classic diathesis-stress, stress-enhancement model of perfectionism with subgroups of Chinese (N = 129) and Asian Indian (N = 166) international graduate students attending a major U.S. university. More specifically, the authors tested whether self-critical perfectionism, accultura-tive stress, and their interaction accounted for different directions of effects and variability in depression. The authors found that self-critical perfectionism was positively associated with depression for both groups, but the effects were stronger for the Asian Indian students. In fact, only for Asian Indians was the Self-Critical Perfectionism × Acculturative Stress interaction signifi-cant, indicating worse depression for those students who were the most self-critical and most stressed. Potential differences in the importance of these predictors for subgroups of international students are discussed. Implications for intervention, prevention, and the need for further testing of these models in longitudinal studies are also addressed. Keywords},
author = {Rice, Kenneth G. and Choi, Chun-Chung and Zhang, Yanmei and Morero, Yanina Ines and Anderson, Debra},
doi = {10.1177/0011000011427061},
