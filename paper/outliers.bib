pmid = {28108977},
publisher = {Psychonomic Bulletin {\&} Review},
title = {{A locus coeruleus-norepinephrine account of individual differences in working memory capacity and attention control}},
url = {http://link.springer.com/10.3758/s13423-016-1220-5},
volume = {24},
year = {2017}
}
