issn = {0047-2891},
journal = {Journal of Youth and Adolescence},
keywords = {Externalizing symptoms,Intergenerational transmission,Internalizing symptoms,Parent-Adolescent attachment,Religiousness},
month = {dec},
number = {12},
pages = {1576--1587},
pmid = {22836938},
title = {{Parent--adolescent relationship quality as a moderator for the influences of parents' religiousness on adolescents' religiousness and adjustment}},
url = {http://link.springer.com/10.1007/s10964-012-9796-1},
volume = {41},
