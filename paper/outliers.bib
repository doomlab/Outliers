}
@article{Kangur2017,
abstract = {The transition from fuel cars to electric cars is a large-scale process involving many interactions between consumers and other stakeholders over decades. To explore how policies may interact with consumer behavior over such a long time period, we developed an agent-based social simulation model. In this model, detailed data of 1795 respondents have been used to parameterise an agent architecture that addresses different consumer needs and decision strategies. Numerical experiments indicate that effective policy requires a long-lasting implementation of a combination of monetary, structural and informational measures. The strongest effect on emission reduction requires an exclusive support for full battery electric cars and no support for hybrid cars.},
author = {Kangur, Ayla and Jager, Wander and Verbrugge, Rineke and Bockarjova, Marija},
doi = {10.1016/j.jenvp.2017.01.002},
issn = {02724944},
journal = {Journal of Environmental Psychology},
keywords = {Agent-based modelling,Diffusion of innovations,Electric vehicles},
month = {oct},
pages = {166--182},
publisher = {Elsevier Ltd},
title = {{An agent-based model for diffusion of electric vehicles}},
url = {http://dx.doi.org/10.1016/j.jenvp.2017.01.002 http://linkinghub.elsevier.com/retrieve/pii/S0272494417300026},
volume = {52},
year = {2017}
}
@article{Grønhøj2017,
abstract = {It is well documented that parents' behaviour and family norms exert a significant influence on young people's pro-environmental attitudes and behaviour. But what is the role of parenting styles in this connection? The present study addresses this question based on a matched sample of young people aged 18–20 (n = 448) and one parent (n = 448), each completing an online questionnaire that included the Motivation Toward the Environment Scale, Perception of Parenting Styles Scale, and questions about perceived (by youth) parental and self-reported (by youth and parent) pro-environmental behaviours. We find that young people display less internalized motivation to ‘do things for the environment' than their parents, but that their motivation to act in pro-environmental ways is rooted in family descriptive norms, parents' internalized motivation to act pro-environmentally, and the autonomy-supporting parenting style and structure prevalent in the home.},
author = {Gr{\o}nh{\o}j, Alice and Th{\o}gersen, John},
doi = {10.1016/j.jenvp.2017.09.005},
issn = {02724944},
journal = {Journal of Environmental Psychology},
month = {dec},
pages = {11--19},
publisher = {Elsevier Ltd},
title = {{Why young people do things for the environment: The role of parenting for adolescents' motivation to engage in pro-environmental behaviour}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S027249441730110X},
volume = {54},
year = {2017}
}
@article{Janssen2016,
abstract = {Behavioral experiments have demonstrated that people do cooperate in commons dilemmas. There are alternative theories that are proposed to explain the data. We will use agent-based models to compare alternative behavioral theories on a series of experimental data of irrigation games. The irrigation dilemma captures situations of asymmetric access to common resources while contributions of all participants are needed to maintain the physical infrastructure.In our model analysis we compare various alternative theories, including na{\"{i}}ve simple ones like selfish rational actors and altruistic actors. We contrast these with various alternative behavioral models for collective action as well as inclusion of other-regarding preferences. The systematic comparison of alternative models on experimental data from 44 groups enables us to test which behavioral theories best explain the observed effects of communication. We do not find that one theory clearly outperform others in explaining the data.},
author = {Janssen, Marco A. and Baggio, Jacopo A.},
doi = {10.1016/j.jenvp.2016.04.003},
issn = {02724944},
journal = {Journal of Environmental Psychology},
keywords = {Agent-based models,Calibration,Common pool resources,Experimental data},
month = {jun},
pages = {106--115},
publisher = {Elsevier Ltd},
title = {{Using agent-based models to compare behavioral theories on experimental data: Application for irrigation games}},
url = {http://dx.doi.org/10.1016/j.jenvp.2016.04.003 http://linkinghub.elsevier.com/retrieve/pii/S0272494416300226},
volume = {46},
year = {2016}
}
@article{Junot2017,
abstract = {Based on the dualistic model of passion and the broaden-and-build theory, this study aimed to test the impact of passion for an outdoor activity on emotions and their effects on affiliation with nature and environmental behaviors. A total of 212 practitioners of outdoor activities from Reunion Island took part in the study. The results of the path analysis showed that harmonious passion was related to positive emotions, which were related to affiliation with nature and ultimately more environmental behaviors. In contrast, obsessive passion was related to negative emotions, which were negatively related to affiliation with nature and environmental behaviors. These results stress the fact that practicing an outdoor activity does not guarantee environmental behaviors. It rather appears that type of passion for these activities matters; and this way, passion can influence emotions and thought-action repertoires of people about nature and environment.},
author = {Junot, Amandine and Paquet, Yvan and Martin-Krumm, Charles},
doi = {10.1016/j.jenvp.2017.07.011},
issn = {02724944},
journal = {Journal of Environmental Psychology},
keywords = {Affiliation with nature,Emotions,Environmental behavior,Outdoor activity,Passion},
month = {nov},
pages = {177--184},
publisher = {Elsevier Ltd},
title = {{Passion for outdoor activities and environmental behaviors: A look at emotions related to passionate activities}},
url = {http://dx.doi.org/10.1016/j.jenvp.2017.07.011 http://linkinghub.elsevier.com/retrieve/pii/S0272494417301007},
volume = {53},
year = {2017}
}
@article{VandenBroek2017,
abstract = {Many environmental campaigns highlight both the environmental and financial benefits of behaviour change, in the hope of motivating a broad audience. But are such mixed appeals more persuasive than separate appeals? We argue that messages tailored to match recipient's prioritised values are more persuasive than combined appeals. We conducted a questionnaire study to assess the persuasiveness of economic, environmental, and mixed appeals (N = 210). As expected, individual differences in values moderated the persuasive power of the different appeals. Importantly, we found that appeals that matched the recipients' values were more persuasive than the combined appeal. Interestingly, personal norms also acted as a moderator. These findings suggest that environmental campaigns aimed to induce behavioural change can benefit from tailoring persuasive messages to idiosyncratic characteristics of their target group, rather than employing a one-size-fits-all message.},
author = {van den Broek, Karlijn and Bolderdijk, Jan Willem and Steg, Linda},
doi = {10.1016/j.jenvp.2017.07.009},
issn = {02724944},
journal = {Journal of Environmental Psychology},
keywords = {Environmental messages,Persuasion,Pro-environmental behaviour,Tailoring,Values},
month = {nov},
pages = {145--156},
publisher = {Elsevier Ltd},
title = {{Individual differences in values determine the relative persuasiveness of biospheric, economic and combined appeals}},
url = {http://dx.doi.org/10.1016/j.jenvp.2017.07.009 http://linkinghub.elsevier.com/retrieve/pii/S0272494417300981},
volume = {53},
year = {2017}
}
