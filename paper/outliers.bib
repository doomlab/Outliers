issn = {1939-1315},
journal = {Journal of Personality and Social Psychology},
keywords = {10,1037,68,according to one recent,an important part of,consider religion to be,culture,doi,dx,estimate,http,of people worldwide,org,pspp0000104,psychological adjustment,religiosity,supp,supplemental materials,their daily lives},
month = {sep},
number = {3},
pages = {18--39},
title = {{The religiosity as social value hypothesis: A multi-method replication and extension across 65 countries and three levels of spatial aggregation.}},
