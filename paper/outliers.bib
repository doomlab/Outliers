journal = {Psychological Methods},
keywords = {behavioral sciences,categorical variables,catpca,in the social and,large number of variables,nonlinear principal components analysis,often confronted with a,pca,researchers are,that they},
number = {3},
pages = {336--358},
pmid = {17784798},
title = {{Nonlinear principal components analysis: Introduction and application.}},
