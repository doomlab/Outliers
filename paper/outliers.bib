}
@article{Wood2016,
abstract = {Although sport is regarded as a bastion of male hegemony, coed settings$\backslash$noffer females the opportunity to compete alongside males. Coed$\backslash$nenvironments, however, often include rule modifications that intend to$\backslash$nfacilitate female participation, which promote female inferiority$\backslash$nassumptions. This study sought to critique modifications in the divisive$\backslash$nworld of coed flag football through the lens of benevolent sexism and$\backslash$nthe shifting standards model. Observational field notes and interview$\backslash$ndata gathered over a season revealed that the equity perceived to be$\backslash$nlegislated by rule modifications was overshadowed by negative outcomes,$\backslash$nas modifications perpetuated stereotypes and reinforced an environment$\backslash$nof male domination. We discuss the ubiquity of male superiority beliefs$\backslash$nwithin coed flag football and implications of the unintended$\backslash$nconsequences of rule modifications.},
