journal = {Journal of Consulting and Clinical Psychology},
keywords = {activation,anxiety disorders,confrontation with feared situations,core element of behavior,exposure,exposure therapy is a,facilitate corrective learning by,fear extinction,means of,or sensa-,physiology,predictor,therapy for,tions is thought to},
